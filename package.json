{
  "name": "kessel-sdk",
  "version": "0.1.0",
  "description": "Client SDK for Kessel.",
  "type": "module",
  "exports": {
    "./*": {
      "require": "./dist/cjs/*",
      "import": "./dist/esm/*",
      "types": "./dist/types/*"
    }
  },
  "scripts": {
    "build": "run-p build:cjs build:esm build:types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs/",
    "build:esm": "tsc --module ES6 --outDir dist/esm/",
    "build:types": "tsc --declaration true --emitDeclarationOnly true --declarationDir dist/types",
    "prettier": "npm run prettier:base -- --write",
    "prettier:check": "npm run prettier:base -- --check",
    "prettier:base": "prettier src/** *.json ./*js README.md examples/**",
    "lint": "eslint --fix src examples",
    "lint:check": "eslint src examples",
    "test": "jest"
  },
  "files": [
    "package.json",
    "dist/**",
    "src/**",
    "README.md",
    "LICENSE.txt"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-kessel/kessel-sdk-node.git"
  },
  "keywords": [
    "kessel"
  ],
  "author": "Josejulio Mart√≠nez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/project-kessel/kessel-sdk-node/issues"
  },
  "homepage": "https://project-kessel.github.io",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.13.5",
    "jest": "^30.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
